	<Position-Y> <!-- Local rotation -->
		<Ring>0.5</Ring>
	</Position-Y>
	<Rotation-X>
		<FallDeadCollision>-90</FallDeadCollision>
	</Rotation-X>
	<Rotation-Y>
		<DashRing>-180</DashRing>
		<JumpBoard>-180</JumpBoard>
		<PointMarker>-180</PointMarker>
		<ChageMode_2DtoForward>-180</ChageMode_2DtoForward>
		<ChageMode_3Dto2D>-180</ChageMode_3Dto2D>
		<ChageMode_3DtoForward>-180</ChageMode_3DtoForward>
		<RainbowRing>-180</RainbowRing>
		<TrickJump>-180</TrickJump>
		<CameraCollisionBoard>-180</CameraCollisionBoard>
	</Rotation-Y>
	<Param-Divide>
		<Collision>5</Collision>
		<Speed>10</Speed>
		<Velocity>10</Velocity>
		<TargetOffset>10</TargetOffset>
		<Distance>10</Distance>
		<Height>10</Height>
		<Width>10</Width>
		<Length>10</Length>
	</Param-Divide>
