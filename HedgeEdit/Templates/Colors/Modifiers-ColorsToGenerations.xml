		<SpringFake>DummySpring</SpringFake>
		<GrindDashPanel>GrindBooster</GrindDashPanel>
		<SelectCanon>JumpSelector</SelectCanon>
		<JumpPole>SpringPole</JumpPole>
		<AdlibTrickJump>TrickJump</AdlibTrickJump>
